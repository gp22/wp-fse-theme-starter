{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "slug": "",
          "color": "",
          "name": ""
        }
      ],
      "gradients": [
        {
          "slug": "",
          "gradient": "",
          "name": ""
        }
      ]
    },
    "spacing": {
      "units": []
    },
    "layout": {
      "contentSize": "42.5rem",
      "wideSize": "clamp(48.5rem, 62vw, 96rem)"
    },
    "typography": {
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Ubuntu\", \"DejaVu Sans\", \"Nimbus Sans L\", \"Helvetica Neue\", sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\", \"Noto Color Emoji\"",
          "name": "System Font",
          "slug": "system-font"
        }
      ],
      "fontSizes": [
        {
          "slug": "tiny",
          "size": "clamp(0.72rem, 0.67rem + 0.23vw, 0.84rem)",
          "name": "Tiny"
        },
        {
          "slug": "small",
          "size": "clamp(0.90rem, 0.84rem + 0.29vw, 1.05rem)",
          "name": "Small"
        },
        {
          "slug": "normal",
          "size": "clamp(1.13rem, 1.05rem + 0.37vw, 1.31rem)",
          "name": "Normal"
        },
        {
          "slug": "medium",
          "size": "clamp(1.41rem, 1.31rem + 0.46vw, 1.64rem)",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "clamp(1.76rem, 1.64rem + 0.57vw, 2.05rem)",
          "name": "Large"
        },
        {
          "slug": "extra-large",
          "size": "clamp(2.20rem, 2.05rem + 0.71vw, 2.56rem)",
          "name": "Extra Large"
        },
        {
          "slug": "huge",
          "size": "clamp(2.75rem, 2.57rem + 0.89vw, 3.20rem)",
          "name": "Huge"
        },
        {
          "slug": "gigantic",
          "size": "clamp(3.43rem, 3.21rem + 1.12vw, 4.01rem)",
          "name": "Gigantic"
        }
      ]
    },
    "custom": {
      "line-height": 1.5,
      "spacing": {
        "body": "0",
        "default": "1.5rem"
      },
      "letter-spacing": {
        "default": "0.125rem",
        "title": "1rem",
        "heading": "0.75rem"
      }
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "var(--wp--custom--spacing--default)",
      "margin": {
        "top": "0px",
        "bottom": "0px"
      }
    },
    "color": {
      "background": "",
      "text": ""
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system-font)",
      "lineHeight": "var(--wp--custom--line-height)",
      "fontSize": "var(--wp--preset--font-size--normal)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-font)",
          "lineHeight": "var(--wp--custom--line-height)",
          "fontSize": "var(--wp--preset--font-size--extra-large)"
        }
      },
      "link": {
        "color": {
          "text": ""
        }
      }
    },
    "blocks": {
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0",
            "bottom": "0"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "",
      "title": "",
      "postTypes": ["post", "page"]
    }
  ]
}
